// Reset player if he falls of the map(Using the Manager Script).
using System.Collections;
using System.Collections.Generic;
using UnityEngine;

public class DeadZone : MonoBehaviour {

	void OnTriggerEnter(Collider Cf){
		if(Cf.gameObject.tag=="Player"){
			Camera.main.GetComponent<Manager>().ResetPlayer();
			Debug.Log ("D");
		}
	}
}
